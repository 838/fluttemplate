  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Beta",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "beta",
        "--dart-define",
        "FLAVOR=beta",
      ]
    },
    {
      "name": "Launch Prod",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define",
        "FLAVOR=prod",
      ]
    },
    {
      "name": "Launch Prod Profile",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define",
        "FLAVOR=prod",
        "--profile"
      ]
    },
    {
      "name": "Launch Prod Release",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "prod",
        "--dart-define",
        "FLAVOR=prod",
        "--release"
      ]
    },
    {
      "name": "Flutter: Attach to Device",
      "type": "dart",
      "request": "attach"
    }
  ]
}